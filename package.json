    "main": "index.js",
    "scripts": {
        "test": "NODE_ENV=test node --test --test-concurrency=1 test/*.test.js",
        "test:auth": "NODE_ENV=test node --test test/auth.test.js",
        "test:bands": "NODE_ENV=test node --test --test-concurrency=1 test/bands.test.js test/transactions-*.test.js",
        "test:integration": "NODE_ENV=test node --test test/google-integration.test.js",
        "start": "test -f .env && node --env-file=.env server.js || node server.js",
        "install-db": "test -f .env && node --env-file=.env install.js || node install.js",
        "postinstall": "test -d data || npm run install-db"
    },
